    @section Styles {
        <link rel="stylesheet" href="~/css/Home/index.css" />
    
        <style>
            body {
                min-height: 200vh;
                background-color: #0A0A0A;
                font-family: "Noto Sans", sans-serif;
            }

            /* ===== HERO SECTION ===== */
            .hero {
                position: relative;
                height: 100vh;
                width: 100%;
                overflow: hidden;
            }

            .bg-video {
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                object-fit: cover;
                z-index: 0;
                filter: brightness(70%);
            }

            .video-overlay {
                position: absolute;
                inset: 0;
                background: linear-gradient(to bottom, rgba(0, 0, 0, 0.3), rgba(0, 0, 0, 0.6));
                z-index: 1;
            }

            .hero h1,
            .hero h2 {
                position: absolute;
                width: 100%;
                text-align: center;
                font-weight: 600;
                opacity: 0;
                transform: translateY(50px);
                z-index: 2;
                letter-spacing: 1px;
                font-size: 4rem;
            }

            .hero h1 {
                top: 32%;
                color:white;
            }

            .hero h2 {
                top: 45%;
                color: #fc4444;
                font-weight: 600;
            }

            /* ===== INFO BOX ===== */
            .info-box {
                position: absolute;
                bottom: -183%;
                left: 50%;
                transform: translateX(-50%);
                width: 36%;
                background: rgba(255, 255, 255, 0.07);
                border: 1px solid rgba(255, 255, 255, 0.1);
                border-radius: 24px;
                padding: 15px;
                text-align: center;
                backdrop-filter: blur(13px);
                z-index: 2;
                box-shadow: 0 10px 25px rgba(0, 0, 0, 0.4);
            }

            .info-video video {
                width: 100%;
                border-radius: 14px;
            }

            .info-text {
                margin-top: 18px;
                font-size: 1rem;
                line-height: 1.6;
                color: #e8e8e8;
                opacity: 0.9;
            }

            /* ===== ACCORDION ===== */
            .accordion {
                margin-top: 25px;
                text-align: left;
            }

            .accordion-item {
                background: rgba(255, 255, 255, 0.05);
                border-radius: 10px;
                margin-bottom: 12px;
                overflow: hidden;
                border: 1px solid rgba(255, 255, 255, 0.1);
                transition: all 0.3s ease;
            }

            .accordion-header {
                width: 100%;
                background: transparent;
                border: none;
                color: #fff;
                font-weight: 500;
                font-size: 1rem;
                padding: 16px 20px;
                text-align: left;
                cursor: pointer;
                transition: background 0.3s;
            }

            .accordion-header:hover {
                background: rgba(255, 255, 255, 0.1);
            }

            .accordion-content {
                display: none;
                padding: 16px 20px;
                background: rgba(255, 255, 255, 0.04);
                font-size: 0.95rem;
                color: #ddd;
                line-height: 1.6;
            }

            .accordion-item.active {
                background: rgba(255, 255, 255, 0.08);
                transform: translateY(-1px);
            }

            .accordion-item.active .accordion-content {
                display: block;
            }

            /* ===== RESPONSIVE ===== */
            @@media (max-width: 768px) {
                .info-box
                {
                    width: 85%;
                    padding: 20px;
                }

                .hero h1 {
                    font-size: 2rem;
                }

                .hero h2 {
                    font-size: 1.2rem;
                }
            }

            /* textcard */




            #main {
                gap: 10vh;
                align-items: center;
                justify-content: center;
                width: 100%;
                min-height: 150vh;
                background-color: #202020;
            }


            .cards {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                width: 80%;
                height: 80vh;
                background-position: center;
                background-size: cover;
